{
    "plugins": [
        "-anchors",
        "insert-md",
        "anchorjs",
        "collapsible-menu",
        "hide-published-with",
        "advanced-emoji",
        "edit-link",
        "localized-footer",
        "page-toc"
    ],
    "pluginsConfig": {
        "insert-md": {
            "md": "![image](http://www.optinomic.com/_logo/optinomic_logo_small.png)\n\n"
        },
        "edit-link": {
            "base": "https://github.com/Optinomic/optinomic-documentation/edit/users/",
            "label": "Edit | Bearbeiten"
        },
        "localized-footer": {
            "filename": "FOOTER.md"
        },
        "page-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "before-first"
        }
    }
}
